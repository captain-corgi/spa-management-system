# Example environment variables for Spa Management System

# PostgreSQL
DATABASE_DSN=postgres://postgres:postgres@localhost:5432/spa?sslmode=disable
DB_MAX_OPEN_CONNS=20
DB_MAX_IDLE_CONNS=10
DB_CONN_MAX_LIFETIME=3600

# Redis
REDIS_ADDR=localhost:6379

# API URLs
VITE_API_URL=http://localhost:8080

# DockerHub (for CI/CD)
DOCKERHUB_USERNAME=
DOCKERHUB_TOKEN=

# Email/Notification
EMAIL_USER=
EMAIL_PASSWORD=
NOTIFY_EMAIL=

# Codecov
CODECOV_TOKEN=
